{
  "name": "@prodobit/config",
  "version": "0.1.2",
  "description": "Configuration management system for Prodobit framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
