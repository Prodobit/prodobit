{
  "name": "@prodobit/sdk",
  "version": "0.7.8",
  "description": "TypeScript SDK for Prodobit API",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:js && pnpm build:types",
    "build:js": "pnpm tsdown",
    "build:types": "echo 'SDK types skipped - arktype needs proper integration'",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting skipped - no linter configured'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prodobit/types": "workspace:*",
    "arktype": "2.1.20"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4"
  },
  "keywords": [
    "prodobit",
    "sdk",
    "typescript",
    "api-client",
    "erp",
    "manufacturing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prodobit/prodobit.git",
    "directory": "packages/sdk"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
