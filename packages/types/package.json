{
  "name": "@prodobit/types",
  "version": "0.6.0",
  "description": "Core types and validation schemas for Prodobit",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:js && pnpm build:types",
    "build:js": "pnpm tsdown",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting skipped - no linter configured'",
    "test": "echo 'Tests skipped - no test framework configured'"
  },
  "dependencies": {
    "arktype": "2.1.20"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "keywords": [
    "prodobit",
    "types",
    "typescript",
    "validation",
    "arktype",
    "schemas"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prodobit/prodobit.git",
    "directory": "packages/types"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
