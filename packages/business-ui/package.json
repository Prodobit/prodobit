{
  "name": "@prodobit/business-ui",
  "version": "0.9.5",
  "description": "Framework-agnostic business UI components for Prodobit (starting with React)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./forms": {
      "import": "./dist/forms/index.js",
      "types": "./dist/forms/index.d.ts"
    },
    "./tables": {
      "import": "./dist/tables/index.js",
      "types": "./dist/tables/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:js && pnpm build:types",
    "build:js": "pnpm tsdown",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck --isolatedModules false",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ark-ui/react": "^5.25.0",
    "@pandacss/dev": "^1.3.0",
    "@park-ui/panda-preset": "^0.43.1",
    "@prodobit/react-sdk": "workspace:*",
    "@prodobit/types": "workspace:*",
    "@tanstack/react-form": "^1.19.5",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "react",
    "vue",
    "svelte",
    "solid",
    "business-ui",
    "forms",
    "tables",
    "tanstack",
    "prodobit",
    "park-ui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prodobit/prodobit.git",
    "directory": "packages/business-ui"
  }
}
